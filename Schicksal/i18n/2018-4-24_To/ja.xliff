  <file original="Schicksal/Supporting Files/en.lproj/Localizable_V_WhiteComet.strings" source-language="en" datatype="plaintext" target-language="ja">
    <header>
      <tool tool-id="com.apple.dt.xcode" tool-name="Xcode" tool-version="9.3" build-num="9E145"/>
